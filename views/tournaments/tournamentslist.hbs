<H2>Tournaments List</H2>
{{#each tournamentList}}
  <div>
    <img
      src='https://miracomosehace.com/wp-content/uploads/2020/08/logo-League-of-Legends.jpg'
      alt=''
      width='200'
      height='100'
    />
    <h3>{{this.name}}</h3>
    <h4>{{this.game}}</h4>

    <a href='/tournaments/{{this._id}}'>More Details</a>

    <a href='/tournaments/{{this._id}}/edit'>Edit tournament</a>
    <a href='/tournaments/{{this._id}}/delete'>Delete tournament</a>

  </div>

{{/each}}